{
  "name": "speedy-shortcuts",
  "displayName": "Speedy Shortcuts",
  "publisher": "bshashikiran",
  "description": "Boost productivity with quick double-tap shortcuts",
  "version": "0.0.2",
  "icon": "images/shift.png",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Keymaps",
    "Other"
  ],
  "keywords": [
    "shortcuts",
    "keyboard",
    "productivity",
    "double-tap",
    "vscode",
    "quick access",
    "fast navigation",
    "hotkeys",
    "workflow",
    "developer tools",
    "shift shift",
    "alt alt",
    "ctrl ctrl"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "workbench.action.findInFiles",
        "key": "shift shift",
        "mac": "shift shift"
      },
      {
        "command": "workbench.action.quickOpen",
        "key": "alt alt",
        "mac": "alt alt"
      },
      {
        "command": "workbench.action.showCommands",
        "key": "ctrl ctrl",
        "mac": "ctrl ctrl"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.54.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}